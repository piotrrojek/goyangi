<button type="button" class="uk-button show-liking-users uk-button-small" data-count="{{likedList.count}}">{{likedList.count}}</button>
<!-- uk-modal -->
<div class="uk-modal" tabindex="-1" aria-hidden="true" id="likingUsersWrapper">
  <div class="uk-modal-dialog">
    <a type="button" class="uk-modal-close uk-close cancel-confirm"></a>
    <div class="uk-modal-header">
      <h4 class="uk-modal-title">{{i18n 'liking.fan.title'}}</h4>
    </div>
    <p class="uk-text-center">
      <div id="likingUsers" class="uk-overflow-container"></div>
      <button type="button" class="uk-button show-more-liking-users uk-hidden">
        {{i18n 'liking.fan.more'}}
      </button>
    </p>
  </div>
  <!-- /.uk-modal-dialog -->
</div>
