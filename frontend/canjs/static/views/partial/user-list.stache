{{#if users}}
<div class="user-wrapper uk-panel uk-panel-box uk-width-medium-1-3 uk-width-large-1-3">
    <h3>{{i18n 'main.users'}} ({{ users.length}})</h3>
    <ul>
        {{#users}}
            <li>
                <a href="/profile/{{ id }}">{{email}}</a>
            </li>
        {{/users}}
    </ul>
</div>
{{/if}}
